				float fresnelNode51 = ( 0.0 + 1.0 * pow( 1.0 - fresnelNdotV51, _Fresnel1 ) );
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( _Color0 * ( tex2DNode29.r + pow( ( tex2DNode29.r * _Tx_d_power ) , _Tx_d_power ) ) ).rgb;
