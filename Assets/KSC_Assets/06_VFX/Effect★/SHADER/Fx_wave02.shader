				float2 texCoord5 = IN.ase_texcoord3.xy * float2( 3,0.5 ) + float2( 0,0 );
				float2 panner9 = ( 1.0 * _Time.y * float2( 0.2,0 ) + texCoord5);
				float2 texCoord16 = IN.ase_texcoord3.xy * float2( 4,1 ) + float2( 0,0 );
