  VideoClipAndDescripts:
  - key: Climbing
    videoClip: {fileID: 32900000, guid: bac7c3d06bd01734bbb7a03366523dd4, type: 3}
    description: "\uBCF8 \uAC8C\uC784\uC5D0\uC11C\uB294 \uD2B9\uC815\uD55C \uD50C\uB7AB\uD3FC\uC5D0
      \uC7A1\uAE30 \uD0A4\uB97C \uC785\uB825\uD558\uBA74 \uD074\uB77C\uC774\uBC0D\uC744
      \uD560 \uC218 \uC788\uC2B5\uB2C8\uB2E4"
  - key: Stamina
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "\uD074\uB77C\uC774\uBC0D\uC744 \uD558\uB294\uB3D9\uC548 \uC774\uB904\uC9C0\uB294
      \uBAA8\uB4E0 \uB3D9\uC791\uC5D0\uB294 \uC545\uB825\uC774 \uC18C\uBAA8\uB429\uB2C8\uB2E4.
      \uD074\uB77C\uC774\uBC0D \uC810\uD504\uB098 \uBCBD \uC810\uD504\uB97C \uD558\uBA74
      \uB354 \uBE60\uB974\uAC8C \uC6C0\uC9C1\uC774\uACE0, \uB04A\uC5B4\uC9C0\uB294
      \uACF3\uC744 \uB118\uC5B4\uAC08 \uC218 \uC788\uC9C0\uB9CC \uC545\uB825\uC774
      \uB354 \uD06C\uAC8C \uC18C\uBAA8\uB418\uBA70, \uC545\uB825\uC744 \uC804\uBD80
      \uC18C\uBAA8\uD558\uBA74 \uB5A8\uC5B4\uC9C0\uAC8C \uB429\uB2C8\uB2E4"
  - key: ClimbingJump
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "\uD074\uB77C\uC774\uBC0D \uC0C1\uD0DC\uC5D0\uC11C\uB294 \uBC29\uD5A5\uD0A4
      + \uC2A4\uD398\uC774\uC2A4\uBC14 \uC785\uB825\uC73C\uB85C \uD074\uB77C\uC774\uBC0D
      \uC810\uD504\uB97C \uD560 \uC218 \uC788\uC73C\uBA70 \uC774\uB97C \uC774\uC6A9\uD574\uC11C
      \uB04A\uC5B4\uC9C4 \uACF3\uC744 \uB118\uC5B4\uAC08 \uC218 \uC788\uC2B5\uB2C8\uB2E4.
      \uB545\uC5D0\uC11C \uC810\uD504\uD560 \uB54C\uC640 \uB2E4\uB974\uAC8C \uC810\uD504\uD0A4\uB97C
      \uB20C\uB800\uB2E4 \uB54C\uB294 \uC2DC\uC810\uC5D0 \uC810\uD504\uB97C \uD558\uB2C8
      \uC8FC\uC758\uD558\uC2DC\uAE38 \uBC14\uB78D\uB2C8\uB2E4"
  - key: ClimbingLeftRightJump
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "\uD074\uB77C\uC774\uBC0D \uC810\uD504\uB294 \uC88C\uC6B0\uB85C\uB3C4
      \uAC00\uB2A5\uD558\uBA70 \uB9C8\uCC2C\uAC00\uC9C0\uB85C \uB04A\uC5B4\uC9C4
      \uACF3\uC744 \uB118\uC5B4\uAC08 \uC218 \uC788\uC2B5\uB2C8\uB2E4"
  - key: WallJump
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "\uD074\uB77C\uC774\uBC0D \uC0C1\uD0DC\uC5D0\uC11C \uBC29\uD5A5\uD0A4
      \uC785\uB825 \uC5C6\uC774 \uC2A4\uD398\uC774\uC2A4\uBC14\uB97C \uC785\uB825\uD558\uBA74
      \uBCBD\uC744 \uBC15\uCC28\uACE0 \uBC18\uB300\uD3B8\uC73C\uB85C \uB6F0\uB294
      \uBCBD \uC810\uD504\uB97C \uD560 \uC218 \uC788\uC2B5\uB2C8\uB2E4"
  - key: WalkRunAndJump
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "\uAC77\uAE30\uC640 \uB2EC\uB9AC\uAE30 \uC9C1\uD6C4 \uC810\uD504\uB294
      \uBE44\uAC70\uB9AC\uC5D0 \uCC28\uC774\uAC00 \uC788\uC2B5\uB2C8\uB2E4"
  - key: AirialControl
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "\uC810\uD504 \uC9C1\uD6C4\uB098 \uC5B4\uB518\uAC00\uC5D0\uC11C
      \uB5A8\uC5B4\uC9C8 \uB54C \uCE90\uB9AD\uD130\uAC00 \uACF5\uC911\uC5D0 \uC788\uB294\uB3D9\uC548
      \uC870\uC791\uD0A4\uB97C \uC774\uC6A9\uD574 \uC704\uCE58\uB97C \uC6C0\uC9C1\uC77C
      \uC218 \uC788\uC2B5\uB2C8\uB2E4. \uC774\uB97C \uC774\uC6A9\uD558\uBA74 \uC7A0\uC2DC
      \uBC1F\uACE0\uC788\uB294 \uD50C\uB7AB\uD3FC\uC744 \uC810\uD504\uB85C \uBC97\uC5B4\uB0AC\uB2E4\uAC00
      \uB2E4\uC2DC \uB3CC\uC544\uC62C\uC218\uB3C4 \uC788\uC2B5\uB2C8\uB2E4, \uB099\uD558\uB300\uBBF8\uC9C0\uB294
      \uC5C6\uC73C\uB2C8 \uC548\uC2EC\uD558\uACE0 \uC870\uC791\uD558\uC2DC\uAE38
      \uBC14\uB78D\uB2C8\uB2E4"
  - key: SidePlatform
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "\uD074\uB77C\uC774\uBC0D\uC874\uC740 \uD615\uD0DC\uAC00 \uD3C9\uBA74
      \uBCBD\uC774 \uC544\uB2D0\uC218\uB3C4 \uC788\uC73C\uBA70 \uD615\uD0DC\uC5D0
      \uB9DE\uCDB0\uC11C \uC785\uCCB4\uC801\uC73C\uB85C\uB3C4 \uD074\uB77C\uC774\uBC0D
      \uC774\uB3D9\uC774 \uAC00\uB2A5\uD569\uB2C8\uB2E4"
  - key: Ledge
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "\uD074\uB77C\uC774\uBC0D\uC911\uC5D0 \uD50C\uB7AB\uD3FC \uB05D\uB2E8\uAE4C\uC9C0
      \uC62C\uB77C\uAC00\uBA74 \uC790\uB3D9\uC73C\uB85C \uB81B\uC9C0 \uC0C1\uD0DC\uAC00
      \uB418\uBA70 \uC774 \uC0C1\uD0DC\uC5D0\uC11C\uB294 \uC870\uC791\uD0A4 \uC785\uB825
      \uC5C6\uC774 \uC2A4\uD398\uC774\uC2A4\uBC14\uB97C \uC785\uB825\uD558\uBA74
      \uBCBD \uC810\uD504 \uB300\uC2E0\uC5D0 \uD50C\uB7AB\uD3FC \uC62C\uB77C\uAC00\uAE30\uB97C
      \uD558\uAC8C \uB429\uB2C8\uB2E4"
  - key: Scan
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "V\uB97C \uB204\uB974\uBA74 \uBC84\uB514\uC5D0\uAC8C \uC2A4\uCE94
      \uB3D9\uC791 \uBA85\uB839\uC744 \uB0B4\uB9B4 \uC218 \uC788\uC2B5\uB2C8\uB2E4.
      \uC2A4\uCE94\uC744 \uC0AC\uC6A9\uD558\uBA74 EMP \uB7F0\uCC98\uB85C \uC601\uD5A5\uC744
      \uC904 \uC218 \uC788\uB294 \uC624\uBE0C\uC81D\uD2B8\uAC00 \uD45C\uC2DC\uB429\uB2C8\uB2E4."
  - key: AimShot
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "C\uD0A4\uB97C \uB204\uB974\uB294 \uB3D9\uC548 EMP\uB7F0\uCC98\uB97C
      \uC870\uC900\uD569\uB2C8\uB2E4. \uC870\uC900\uC2DC\uC5D0\uB294 \uC5D0\uB108\uC9C0\uB97C
      \uC790\uB3D9\uC73C\uB85C \uCC28\uC9D5\uD558\uBA70 \uCC28\uC9D5 1\uB2E8\uACC4
      \uC774\uC0C1\uC774 \uB418\uC5B4\uC57C \uC0AC\uACA9\uC774 \uAC00\uB2A5\uD569\uB2C8\uB2E4.
      \uB610\uD55C \uAC01 \uC0AC\uACA9\uD6C4 \uB2E4\uC2DC \uCC28\uC9D5\uC774 \uC2DC\uC791\uB418\uAE30\uAE4C\uC9C0
      1\uCD08\uC758 \uD140\uC774 \uC788\uC2B5\uB2C8\uB2E4."
  - key: AimTip
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "\uC870\uC900 \uC0C1\uD0DC\uC5D0\uC11C\uB294 \uB2EC\uB9AC\uAE30\uC640
      \uC810\uD504\uB97C \uBABB\uD558\uBA70 \uC774\uB3D9\uC18D\uB3C4\uAC00 \uB290\uB824\uC9D1\uB2C8\uB2E4.
      \uD558\uC9C0\uB9CC, \uC810\uD504 \uC9C1\uD6C4\uB97C \uD3EC\uD568\uD55C \uACF5\uC911\uC5D0\uC11C\uB3C4
      \uC870\uC900 \uC0C1\uD0DC\uC5D0 \uB4E4\uC5B4\uAC08 \uC218 \uC788\uAE30 \uB54C\uBB38\uC5D0
      \uB099\uD558 \uBC0F \uC810\uD504 \uC9C1\uD6C4\uC5D0\uB3C4 \uC870\uC900\uC5D0
      \uB4E4\uC5B4\uAC04 \uCC44\uB85C \uCC29\uC9C0\uAC00 \uAC00\uB2A5\uD569\uB2C8\uB2E4."
  - key: UseEnergy
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "\uC5D0\uB108\uC9C0 \uCC28\uC9D5\uC740 \uC870\uC900 \uD6C4 \uC77C\uC815
      \uC2DC\uAC04\uB9C8\uB2E4 \uCC28\uC9D5 \uB2E8\uACC4\uAC00 \uC62C\uB77C\uAC00\uBA70
      \uB2E8\uACC4\uBCC4 \uACF5\uACA9\uB825\uACFC \uC18C\uBAA8 \uC5D0\uB108\uC9C0\uB7C9\uC774
      \uB3D9\uC77C\uD558\uAC8C \uC0C1\uC2B9\uD569\uB2C8\uB2E4. \uCC28\uC9D5 \uB2E8\uACC4
      \uC0C1\uC2B9\uC740 EMP \uB7F0\uCC98 \uC6B0\uCE21\uC5D0 \uBD99\uC5B4\uC788\uB294
      UI\uC640 \uC5D0\uC784 \uBAA8\uC591 \uBCC0\uD654\uB85C \uC54C \uC218 \uC788\uC2B5\uB2C8\uB2E4."
  - key: RestoreEnergy
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "EMP \uB7F0\uCC98\uC758 \uC5D0\uB108\uC9C0\uB294 \uC6B4\uB3D9 \uC5D0\uB108\uC9C0\uB97C
      \uBCC0\uD658\uD574\uC11C \uCC44\uC6CC\uC9D1\uB2C8\uB2E4. \uBAA8\uB4E0 \uC774\uB3D9
      \uC870\uC791\uC73C\uB85C \uCC44\uC6CC\uC9C0\uBA70 \uC870\uC900 \uC0C1\uD0DC\uC5D0\uC11C\uB3C4
      \uB290\uB9AC\uC9C0\uB9CC \uCC44\uC6B8 \uC218 \uC788\uC2B5\uB2C8\uB2E4. \uC870\uC900
      \uC0C1\uD0DC\uAC00 \uB418\uBA74 \uC790\uB3D9\uC73C\uB85C \uCC28\uC9D5\uC774
      \uB418\uC9C0\uB9CC \uC3D8\uC9C0 \uC54A\uB294\uB2E4\uBA74 \uC5D0\uB108\uC9C0\uB97C
      \uC18C\uBAA8\uD558\uC9C0 \uC54A\uC2B5\uB2C8\uB2E4."
  - key: Shield
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "\uD0C0\uB974\uD0C0\uB85C\uC2A4\uC758 \uAC1C\uCCB4\uB4E4\uC740 \uB300\uBD80\uBD84
      \uB3D9\uB825\uC6D0 \uC5ED\uD560\uC744 \uD558\uB294 '\uC57D\uC810'\uC744 \uAC00\uC9C0\uACE0
      \uC788\uC73C\uBA70 \uD574\uB2F9 \uC57D\uC810\uC740 '\uC2E4\uB4DC'\uC758 \uBCF4\uD638\uB97C
      \uBC1B\uACE0 \uC788\uC2B5\uB2C8\uB2E4. \uC57D\uC810\uC740 \uB300\uBBF8\uC9C0\uC758
      \uAC15\uB3C4\uC640 \uC0C1\uAD00     \uC5C6\uC774 \uACF5\uACA9\uC744 \uBC1B\uC73C\uBA74
      \uBC14\uB85C \uBD95\uAD34\uB418\uBA70 \uC2E4\uB4DC\uB294 HP\uB97C \uAC00\uC9C0\uACE0
      \uC788\uC2B5\uB2C8\uB2E4. \uC2E4\uB4DC\uC758 \uB0A8\uC740 HP\uB294 \uC0C9\uAE54\uACFC
      \uD45C\uBA74\uC758 \uAE68\uC9C4 \uC0C1\uD0DC\uB85C \uC54C \uC218 \uC788\uC2B5\uB2C8\uB2E4."
  - key: EmpOver
    videoClip: {fileID: 32900000, guid: 27a80db3328e4e440af70a8e766fbce7, type: 3}
    description: "EMP \uB7F0\uCC98\uC758 \uCC28\uC9D5 3\uB2E8\uACC4\uB294 \uCC28\uC9D5
      \uB9C8\uC9C0\uB9C9 \uB2E8\uACC4\uC778 \uB9CC\uD07C \uC81C\uC77C \uC624\uB798
      \uAC78\uB9AC\uC9C0\uB9CC \uD55C \uBC88\uC758 \uACF5\uACA9\uC73C\uB85C \uCD5C\uB300
      HP \uC0C1\uD0DC\uC778 \uC2E4\uB4DC\uB3C4 \uD55C \uBC88\uC5D0 \uAE68\uD2B8\uB9B4
      \uC218 \uC788\uC2B5\uB2C8\uB2E4. \uB300\uC2E0, \uC624\uB798 \uCC28\uC9D5\uD55C
      \uB9CC\uD07C \uC3D8\uC9C0 \uC54A\uACE0 \uC870\uC900\uC744 \uD480\uBA74 EMP
      \uB7F0\uCC98\uC5D0 \uACFC\uBD80\uD558\uAC00 \uAC78\uB824 \uC7A0\uC2DC \uB3D9\uC548
      \uCC28\uC9D5 \uC18D\uB3C4\uAC00 \uBC18\uAC10\uB429\uB2C8\uB2E4."
